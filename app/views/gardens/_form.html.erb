<div class="gardens-form">
  <%= simple_form_for(garden) do |f| %>
    <%= f.input :name %>
    <%= f.input :street %>
    <%= f.input :number %>
    <%= f.input :zipcode %>
    <%= f.input :country %>
    <%= f.input :capacity %>
    <%= f.input :price %>
    <%= f.input :description %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>
</div>
